<%#= d.object %>
<div class="nested-fields well well-compact">
  <div class="field">
     <%= f.label :ディスク名 %>
     <%= f.text_field :disc_name %>
  </div>
  <div class="field">
     <%= f.label :ディスク番号 %>
     <%= f.number_field :disc_number %><br>
  </div>

  <div class="songs">
    <%= f.fields_for :songs do |song| %>
      <%= render 'song_fields', d: song %>
    <%end%>
    <div class="links">
      <%= link_to_add_association "add song", f, :songs %>
      <!--@ﾌﾄｼさん_原因不明エラーです。多分変数関連-->
    </div>
  </div>
</div>