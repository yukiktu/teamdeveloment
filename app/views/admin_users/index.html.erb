
<div class="admain">
	<div class="aduser-search">
        <%= form_tag({:controller => 'admin_users', :action => 'search'}, {:method => :get}) do %>
			<%= text_field_tag 'keywords' %>
			<%= submit_tag '検索', class: "entry_field11" %>
		<% end %>
    </div>

<table class="adtable">
	<thead>
		<tr class="adh">
			<th class="adsimei">氏名</th>
			<th class="add">メールアドレス</th>
			<th class="adlogin">最終ログイン</th>
			<th class="adstatus1">ステータス</th>
		</tr>
	</thead>
	<% @end_users.each do |e| %>
	<tbody>
		<tr class="adb">
			<td class="adname"><%= link_to(admin_user_path(e.id)) do %>
					<%= e.last_name %><%= e.first_name %></td>
				<% end %>
			<td class="admail"><%= e.email %></td>
			<td class="adtime"><%= e.updated_at.strftime("%Y-%m-%d") %></td>
			<td class="adstatus">
				<% if e.enrollment_status == 0 %>
						退会済
					<% else  %>
						在籍中
					<% end %>
			</td>

			<!-- <tb class="editbotan"><%#= link_to "編集", edit_end_user_path(e.id) %></tb> -->

		    </tr>
	    </tbody>
	<% end %>
  </table>
</div>

<div class="page"><%= paginate(@end_users) %></div>