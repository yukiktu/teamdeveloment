<h1>税率・送料一覧</h1>

 <% @tax_rate.each do |t| %>

   <table class="tax">
  	<thead>
      <tr>
        <th>税率</th>
        <th>更新日</th>
      </tr>
    </thead>


    <tbody>
      <tr>
        <td><%= t.tax_rate %></td>
        <td><%= t.created_at.strftime('%Y/%m/%d %H:%M') %></td>
      </tr>
    </tbody>
   </table>
  <% end %>

  <% @shipping_fee.each do |s| %>

   <table class="fee">
  	<thead>
       <tr>
        <th>送料</th>
        <th>更新日</th>
       </tr>
    </thead>

    <tbody>
      <tr>
        <td><%= s.shipping_fee %></td>
        <td><%= s.created_at.strftime('%Y/%m/%d %H:%M') %></td>
      </tr>
    </tbody>
   </table>
  <% end %>